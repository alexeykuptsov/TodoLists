{
  "name": "todo-lists-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "buildDev": "vue-cli-service build --mode development",
    "buildDevWatch": "vue-cli-service build --mode development --watch",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.38.1",
    "devextreme": "^23.2.9",
    "devextreme-vue": "^23.2.9",
    "jquery": "^3.7.1",
    "splitpanes": "^3.1.5",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/eslint-parser": "^7.25.8",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "eslint": "^v8.57.0",
    "eslint-plugin-vue": "^9.29.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unused-vars": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": "vue-cli-service lint"
  }
}
